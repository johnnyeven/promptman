{
    "extends": [
        "next",
        "next/core-web-vitals",
        "eslint:recommended"
    ],
    "globals": {
        "React": "readonly"
    },
    "overrides": [],
    "rules": {
        "no-unused-vars": [
            1,
            {
                "args": "after-used",
                "argsIgnorePattern": "^_"
            }
        ],
        "react/no-unescaped-entities": "off"
    }
}